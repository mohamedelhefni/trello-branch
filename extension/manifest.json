{
  "name": "Trello Branch",
  "description": "Trello Branch Selector",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.trello.com/*"],
      "js": ["content.js"]
    }
  ],

  "permissions": ["storage", "activeTab", "scripting"]
}
